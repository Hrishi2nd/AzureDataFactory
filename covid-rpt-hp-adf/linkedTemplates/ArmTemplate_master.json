{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-rpt-hp-adf"
		},
		"ls_ablob_covidreportingsa2145_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsa2145'"
		},
		"ls_adls_covidreportdl2145_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportdl2145'"
		},
		"ls_adls_covidreportdl2145_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportdl2145.dfs.core.windows.net/"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4bcd7908-5af3-444d-ba38-86d7c2b280f7/resourceGroups/covid-reporting-hp-rg/providers/Microsoft.Storage/storageAccounts/covidreportingsa2145"
		},
		"ls_ecdc_europa_eu_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-rpt-hp-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsa2145_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsa2145_connectionString')]"
					},
					"ls_adls_covidreportdl2145_accountKey": {
						"value": "[parameters('ls_adls_covidreportdl2145_accountKey')]"
					},
					"ls_adls_covidreportdl2145_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportdl2145_properties_typeProperties_url')]"
					},
					"ls_ecdc_europa_eu_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_europa_eu_http_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
